/* 정리된 코드 */

public static String renderPageWithSetupsAndTeardowns(PageData pageData, boolean isSuite) throws Exception { 

    if (isTestPage(pageData))

      includeSetupAndTeardownPages(pageData, isSuite); 

    return pageData.getHtml();

}
